<head>
 {% block stylesheets %}
	<!-- Favicons-->
	<link rel="shortcut icon" href={{ asset('img/favicon.png')}} type="image/x-icon">
	<link rel="apple-touch-icon" type="image/x-icon" href="img/apple-touch-icon-57x57-precomposed.png">
	<link rel="apple-touch-icon" type="image/x-icon" sizes="72x72" href="img/apple-touch-icon-72x72-precomposed.png">
	<link rel="apple-touch-icon" type="image/x-icon" sizes="114x114" href="img/apple-touch-icon-114x114-precomposed.png">
	<link rel="apple-touch-icon" type="image/x-icon" sizes="144x144" href="img/apple-touch-icon-144x144-precomposed.png">
    
    <!-- GOOGLE WEB FONT -->
    <link href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700,800" rel="stylesheet">

	<!-- BASE CSS -->
	<link href={{ asset('css/bootstrap.min.css') }} rel="stylesheet">
	<link href={{ asset('css/style.css') }} rel="stylesheet">
	<link href={{ asset('css/menu.css') }} rel="stylesheet">
	<link href={{ asset('css/vendors.css') }} rel="stylesheet">
	<link href={{ asset('css/icon_fonts/css/all_icons_min.css') }} rel="stylesheet">
    
	<!-- YOUR CUSTOM CSS -->
	<link href{{ asset('css/custom.css')}} rel="stylesheet">
    {% endblock %}
    </head>
    {# Customized Sliding pagination control implementation #}
            {% if pageCount > 1 %}
            <nav aria-label="" class="add_top_20">

						<ul class="pagination pagination-sm">
                        {% if previous is defined %}
							<li class="page-item">
								<a class="page-link" href="{{ path(route, query|merge({(pageParameterName): previous})) }}" tabindex="-1">Precedent</a>
							</li>
                        {% endif %}
                        {% for page in pagesInRange %}
                         {% if page != current %}
							<li class="page-item"><a class="page-link" href="{{ path(route, query|merge({(pageParameterName): page})) }}">{{ page }}</a></li>
                        {% else %}
                        <li class="page-item active current"><a class="page-link" href="#">{{ page }}</a></li>
                        {% endif %}
                        {% endfor %}
                        {% if next is defined %}
                        <li class="page-item">
                            <a class="page-link" href="{{ path(route, query|merge({(pageParameterName): next})) }}">Suivant</a>
                        </li>
                        {% endif %}
                        {% if last is defined and current != last %}
                        <li class="page-item ">
                                <a class="page-link" href="{{ path(route, query|merge({(pageParameterName): last})) }}" tabindex="-1">Dernier</a>
                            </li>
                        {% endif %}
                        {% endif %}
						</ul>
</nav>
